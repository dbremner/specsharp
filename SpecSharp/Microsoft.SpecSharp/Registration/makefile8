clean:
  if exist Binaries\Microsoft.VisualStudio.Package.dll regasm /nologo /silent /u Binaries\Microsoft.VisualStudio.Package.dll
  if exist Binaries\Microsoft.SpecSharp.dll regasm /nologo /silent /u Binaries\Microsoft.SpecSharp.dll
  if exist Binaries\ContractsPropertyPane.dll regasm /nologo /silent /u Binaries\ContractsPropertyPane.dll
  if exist Binaries\*.dll del /Q Binaries\*.dll
  if exist Binaries\*.dll.temp del /Q Binaries\*.dll.temp
  if exist Binaries\*.pdb del /Q Binaries\*.pdb
  if exist Binaries\*.pdb.temp del /Q Binaries\*.pdb.temp
  if exist Binaries\*.exe del /Q Binaries\*.exe
  if exist Binaries\*.exe.temp del /Q Binaries\*.exe.temp
  if exist Binaries\1033\*.dll del /Q Binaries\1033\*.dll
  if exist Binaries\1033\*.dll.temp del /Q Binaries\1033\*.dll.temp
  if exist Binaries\1033\*.pdb del /Q Binaries\1033\*.pdb
